{
  "name": "@monke-say/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "uvicorn src.main:app --reload --host 0.0.0.0 --port 8000",
    "start": "uvicorn src.main:app --host 0.0.0.0 --port 8000",
    "db:migrate": "cd src && alembic upgrade head",
    "db:revision": "cd src && alembic revision --autogenerate -m",
    "db:seed": "python -m src.db.seed"
  }
}
